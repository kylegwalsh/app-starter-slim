{
  "name": "my-app",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "sst dev",
    "backend": "pnpm --filter backend",
    "web": "pnpm --filter web",
    "design": "pnpm --filter design",
    "build": "sst build",
    "deploy": "pnpm backend db:generate && sst deploy",
    "unlock": "sst unlock",
    "test": "turbo test --continue",
    "test:ci": "turbo test:ci --continue",
    "lint": "turbo lint --continue",
    "types:check": "turbo types:check --continue",
    "format": "turbo format --continue",
    "init": "tsx scripts/init/init",
    "init:existing": "tsx scripts/init/init-existing",
    "update-readme": "doctoc README.md",
    "get-package-version": "tsx scripts/get-package-version",
    "create:package": "tsx scripts/create-package",
    "postinstall": "manypkg fix",
    "pre-commit": "tsx scripts/pre-commit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.24.0",
    "@repo/eslint": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@types/aws-lambda": "8.10.149",
    "@vitest/ui": "^3.1.4",
    "axios": "^1.9.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "doctoc": "2.2.0",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "inquirer": "^9.2.16",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sst": "3.17.10",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "zod": "3.25.46"
  }
}
